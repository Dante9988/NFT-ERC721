{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Run Artillery Test",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/node_modules/.bin/artillery",
            "args": [
                "run",
                "${workspaceFolder}/performance/src/scenarios/nft-tests.yml"
            ],
            "console": "integratedTerminal",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "sourceMaps": true,
            "envFile": "${workspaceFolder}/.env"
        }
    ]
} 